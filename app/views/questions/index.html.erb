   <div class="wrapper" >
     <div class="row contentWidth">
        
        <div class="bread col-9 col-s-10">
         <div class="bread__inner">
           <ul>
             <li class="bread__li"><%= link_to "Home", root_path %></li>
             <li class="bread__li"><%= link_to "category", categories_path %></li>
             <li class="bread__li"><%= link_to "Vscode", categories_path %></li>
             <!--<li class="bread__li"><%= link_to "Vscode", categories_path %></li>-->
           </ul>
         </div>
        </div>

           <div class="level col-3 col-s-10 pl-s10 pr-s10">
             <div class="dispPoint">
               <div class="row">
                 <div class="col-5">
                  <p class="user_name"><%= @user.name %></p>
                  <p class="user_name">Lv. <%= @user.level%></p>
                 </div>
                 <div class="col-7">
                  <p class="my_cat_rank">☆☆☆☆☆</p>
                  <p class="my_point">EX. <%= @user.experience_point %></p>
                 </div>
               </div>
             </div>
           </div>
          </div>
          <div class="col-12 h-50 w-100" >
            <div class="mx-auto test-box">
          <div class="container row" style="margin-top: 20px, position-relative;">


            <div class="leftnav col-2">
              <div class="col-xs-12">
                <h3>Command</h3>
              </div>
                <p>⌘ - Command</p>
                <p>⌥ - Option</p>
                <p> ⇧ - Shift</p>
                <p>⌃ - Control</p>
                <p>⏎ - Enter </p>
                <p>/Delete Fn + ⌫</p>
            </div>
            
              <div class="col-8 justify-content">
                <div class="main__inner">
                    <div class="question__wrap">
                       <% @questions_json.each_with_index do |question, idx| %>
                        <div id="question-<%= idx %>" class="questions-item" style="<%= idx == 0 ? '' : 'display: none;' %>">
                          <span class="problem">問題 :<%= question.problem %></sapn>
                          <% sum = 1 %>
                          <span><%= sum += idx %> / 10</span>
                            <div class="previewArea__wrap">
                               <%= attachment_image_tag question, :problem_image, size: "500x300", fallback: "giphy.gif" %>
                            </div>
                          <span id="display"><%= question.display_key %></span>
                          <!--<span id="right" style='display: none;'></span>-->
                        </div>
                    </div>
                      <% end %>
                       <div class="rightnav col-2 col-xs-5">
                <div class="rightnav__inner">
                  <h2>注意コマンド</h2>
                  <div class="preList_wrap">
                    <ul class="preList" style="top: 8opx;">
                      <li>Mac下記を同時に押さないでください</li>
                      <li>⌘ + W, ⌘ + Q, </li>
                      <span id="right" style='display: none;'></span>
                      <li><div id="answer"></div></li>
                    </ul>
                  </div>
                  <div id="switch"></div>
                    <div class="preList_wrap">
                      <h4>各コマンド説明はこちら！↓↓↓↓</h4>
                        <div class="mt20"></div>
                          <aside class>
                            <ul class>
                              <li class="sidebar__rank__li">
                                <%= link_to "コマンド一覧", commands_path %>
                              </li>
                            </ul>
                          </aside>
                        </div>
                    </div>
                  </div>
                </div>
               </div>
                  <div class="inputarea">
                   <div id="input"></div>
                   <div id="command"></div>
                  </div>
              </div>
        

